{
  "name": "scaffold-stylus-template",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace @scaffold-stylus/frontend dev",
    "frontend:dev": "yarn workspace @scaffold-stylus/frontend dev",
    "frontend:build": "yarn workspace @scaffold-stylus/frontend build",
    "frontend:preview": "yarn workspace @scaffold-stylus/frontend preview",
    "contracts:build": "cargo build --manifest-path contracts/Cargo.toml",
    "contracts:test": "cargo test --manifest-path contracts/Cargo.toml",
    "contracts:abi": "cargo build --manifest-path contracts/Cargo.toml --features export-abi"
  },
  "dependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "ws": "8.19.1",
    "@ethersproject/providers": {
      "ws": "7.5.10"
    }
  }
}
